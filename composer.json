{
    "name": "danielkulbe/shariff",
    "description": "Shariff enables website users to share their favorite content without compromising their privacy.",
    "type": "bolt-extension",
    "keywords": [
        "share", "social", "google", "google+", "facebook", "twitter", "whatsapp", "mail"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Kulbe",
            "email": "info@daniel-kulbe.de"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "http://gitlab.daniel-kulbe.de/root/bolt-shariff.git"
        }
    ],
    "require": {
        "bolt/bolt": ">=2.2.0,<3.0.0",
        "php": ">=5.3"
    },
    "autoload": {
        "files": [
            "init.php"
        ],
        "psr-4": {
            "Bolt\\Extension\\DanielKulbe\\Shariff\\": [
                "",
                "src/"
            ],
            "Bolt\\Extension\\DanielKulbe\\Shariff\\Backend\\" : [
                "src/Backend/"
            ]
        }
    },
    "extra": {
        "bolt-assets": "assets",
        "bolt-icon": "assets/shariff.png",
        "branch-alias" : {
            "dev-master" : "1.0.x-dev"
        }
    }
}